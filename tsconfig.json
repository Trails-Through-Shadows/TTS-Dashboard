{
  "compilerOptions": {
    "outFile": "static/js/dashboard.js",
    "allowJs": true,
    "checkJs": true,
    "declaration": false,
    "sourceMap": true,
    "module": "amd",
    "target": "es5",
    "lib": ["es5", "es6", "dom"],
    "paths": {
      "*": ["node_modules/*", "static/js/src/*"]
    }
  },
  "include": [
    "node_modules/**/*.d.ts",
    "static/js/src/**/*.ts"
  ]
}